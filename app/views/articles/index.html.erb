<h1>All Articles</h1>
<h2><%= link_to "All Tags", tags_path %>
<ul id="articles">
  <% @articles.reverse.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article), class: 'article_title', id: "article_#{article.id}" %><br>
    </li>
  <% end %>
</ul>
<%= link_to "Create New Article", new_article_path, class: "new_article" %>